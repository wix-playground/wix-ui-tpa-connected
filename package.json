{
  "name": "wix-ui-tpa-connected",
  "version": "2.0.0",
  "private": true,
  "description": "Connection of Wix UI TPA to settings made simple",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run generate",
    "generate": "./node_modules/.bin/wutc-generator generate || true && rm -rf ./cache/generator",
    "clean": "find *.js | grep -v jest.config.js | xargs rm || true && rm -rf ./cache || true",
    "clean-install": "npm run clean && rm -rf ./node_modules || true && rm ./package-lock.json || true && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix-playground/wix-ui-tpa-connected.git"
  },
  "keywords": [
    "WIX",
    "UI",
    "TPA",
    "settings",
    "connected",
    "color",
    "colors",
    "font",
    "fonts",
    "wrapper"
  ],
  "author": "Vytenis Urbonavicius",
  "license": "MIT",
  "homepage": "https://github.com/wix-playground/wix-ui-tpa-connected",
  "dependencies": {
    "wix-ui-tpa-analyser": "^1.0.0"
  },
  "devDependencies": {
    "@stylable/webpack-plugin": "^1.1.4",
    "find-node-modules": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "svg-inline-loader": "^0.8.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "wix-ui-tpa": "^1.0.119",
    "wix-ui-tpa-connected-generator": "^1.1.6"
  },
  "publishConfig": {}
}
